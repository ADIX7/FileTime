<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="AppBackgroundColor">#FFFFFF</Color>
    <Color x:Key="ContainerBackgroundColor">#EEEEEE</Color>

    <Color x:Key="ItemBackgroundColor">#EEEEEE</Color>
    <Color x:Key="AlternativeItemBackgroundColor">#E0E0E0E0</Color>
    <Color x:Key="SelectedItemBackgroundColor">#AAAAAA</Color>

    <Color x:Key="ForegroundColor">#000000</Color>
    <Color x:Key="AlternativeForegroundColor">#000000</Color>
    <Color x:Key="SelectedItemForegroundColor">#EEEEEE</Color>

    <Color x:Key="ErrorColor">#dc322f</Color>

    <LinearGradientBrush x:Key="ContentSeparatorBrush">
        <GradientStop Offset="0" Color="#00000000" />
        <GradientStop Offset="0.5" Color="#000000" />
        <GradientStop Offset="1" Color="#00000000" />
    </LinearGradientBrush>
</ResourceDictionary>
